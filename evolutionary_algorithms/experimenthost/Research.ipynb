    "# Functional Programming\n",
    "\n",
    "def multiply_num(x1, x2):\n",
    "    return x1 * x2\n",
    "\n",
    "def sum_num(x1, x2, x3):\n",
    "    return x1 + x2 + x3\n",
    "\n",
    "def multiply_sum(sums, multiply):\n",
    "    print(*sums)\n",
    "    print(sum_num(*sums))\n",
    "    print(multiply_num(*multiply))\n",
    "    \n",
    "    "
